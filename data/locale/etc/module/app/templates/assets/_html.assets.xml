<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<assets name="Foundation assets">
		<header>
			<styles>
				<iconfonts>
					<file>/assets/customer/css/font-awesome.css</file>
					<media>screen</media>
					<rel>stylesheet</rel>
					<type>text/css</type>
				</iconfonts>
				<normalize>
					<file>/assets/customer/css/foundation/normalize.css</file>
					<media>screen</media>
					<rel>stylesheet</rel>
					<type>text/css</type>
				</normalize>
				<foundation>
					<file>/assets/customer/css/foundation/foundation.min.css</file>
					<media>screen</media>
					<rel>stylesheet</rel>
					<type>text/css</type>
				</foundation>
				<dev>
					<file>/assets/customer/css/customer.css</file>
					<rel>stylesheet</rel>
					<type>text/css</type>
					<attr>
					<media>screen</media>
					</attr>
				</dev>					
			</styles>
		</header>
		<footer>
			<scripts>
				<require>
					<file>/assets/js/requirejs/require.js</file>
					<type>text/javascript</type>
				</require>
				<app>
					<file>/assets/js/app.js</file>
					<type>text/javascript</type>
				</app>
			</scripts>
		</footer>		
	</assets>
	<bootstrapassets name="Bootstrap assets">
		<header>
			<styles>
				<iconfonts>
					<file>/assets/customer/css/font-awesome.css</file>
					<media>screen</media>
					<rel>stylesheet</rel>
					<type>text/css</type>
				</iconfonts>
				<bootstrap>
					<file>/assets/customer/css/bootstrap/bootstrap.min.css</file>
					<media>screen</media>
					<rel>stylesheet</rel>
					<type>text/css</type>				
				</bootstrap>			
			</styles>
		</header>
		<footer>
			<scripts>
				<require>
					<file>/assets/js/requirejs/require.js</file>
					<type>text/javascript</type>
				</require>
				<app>
					<file>/assets/js/app-bootstrap.js</file>
					<type>text/javascript</type>
				</app>
			</scripts>
		</footer>
	</bootstrapassets>
</layout>